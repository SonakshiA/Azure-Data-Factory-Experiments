{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azdft-adf-demo"
		},
		"ls_sqldb_dw_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azdft-sql-internal-demo.database.windows.net"
		},
		"ls_sqldb_dw_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azdft-sqldb-dw-demo"
		},
		"ls_st_stage_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://azdftststagedemo.blob.core.windows.net"
		},
		"trg_tutorial004_demo1_storage_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ea86e4d9-1558-4b5f-8117-6b5b1dffdd84/resourceGroups/azdft-rg-demo/providers/Microsoft.Storage/storageAccounts/azdftststagedemo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azdft-adf-demo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sqldb_dw_properties_typeProperties_server": {
						"value": "[parameters('ls_sqldb_dw_properties_typeProperties_server')]"
					},
					"ls_sqldb_dw_properties_typeProperties_database": {
						"value": "[parameters('ls_sqldb_dw_properties_typeProperties_database')]"
					},
					"ls_st_stage_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ls_st_stage_properties_typeProperties_serviceEndpoint')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "azdft-adf-demo_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trg_tutorial004_demo1_storage_properties_typeProperties_scope": {
						"value": "[parameters('trg_tutorial004_demo1_storage_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azdft-adf-demo_ArmTemplate_0')]"
			]
		}
	]
}